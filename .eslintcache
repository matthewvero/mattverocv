ory.push('/projects')\n                                                }\n                                          >\n                                                <SubTitle\n                                                      $location={\n                                                            location.pathname ===\n                                                            '/projects'\n                                                      }\n                                                >\n                                                      Projects\n                                                </SubTitle>\n                                          </MainMenuItemTouch>\n\n                                          <HR />\n                                    </MainMenuSlide>\n                              </CSSTransition>\n                        </MainMenuContainer>\n                  </CSSTransition>\n            </MainMenuShell>\n      )\n}\n\nexport default withRouter(MainMenu)\n","/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/components/main-menu/main-menu.styles.js",[],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/redux.js",[],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/pages/homepage/homepage.jsx",["129"],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/pages/homepage/homepage.styles.js",[],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/ui-custom-hooks.js",["130"],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/theme.js",["131"],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/pages/aboutme/about-me-page.component.jsx",[],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/pages/aboutme/about-me-page.styles.js",[],"/Users/matthewvero/OneDrive/Documents/Coding/CV/mycv/src/HOCs/with-touch-animator.js",["132"],"/** @format */\n\nimport React, { useEffect, useState } from 'react'\n\nconst withTouchAnimator = (WrappedComponent) => {\n      return ({ children, fn, $re